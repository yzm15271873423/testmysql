<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example20230223.testmysql.com.yzm.mapper.UserMapper">
    <resultMap id="userMapper" type="com.example20230223.testmysql.com.yzm.pojo.User">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="age" column="age"/>
    </resultMap>


    <select id="selectAll" resultMap="userMapper" >
        select id, name, age from emp;
    </select>
</mapper>